<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kelin.multitypeadapterdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_android"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".SingleTypeListActivity"/>

        <activity android:name=".MultiTypeListActivity"/>

        <activity android:name=".LoadMoreListActivity"/>

        <activity android:name=".FloatListActivity"/>

    </application>

</manifest>